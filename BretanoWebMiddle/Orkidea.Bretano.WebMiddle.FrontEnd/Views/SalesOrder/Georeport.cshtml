@{
    ViewBag.Title = "Georeport";
}
<script src="~/Scripts/Custom/ordrGeoreport.js"></script>
<h2>Reporte geográfico</h2>
<div class="row">
    <div class="form-group">
        <div class="col-md-2">
            @Html.Label("Desde", new { @for = "start", @class = "control-label" })
            <div class="input-group date dtPicker" id="datetimepicker1">
                <input type="text" class="form-control" id="start" />
                <span class="input-group-addon">
                    <span class="fa fa-calendar"></span>
                </span>
            </div>
        </div>
        <div class="col-md-2">
            @Html.Label("Hasta", new { @for = "end", @class = "control-label" })
            <div class="input-group date dtPicker" id="datetimepicker2">
                <input type="text" class="form-control" id="end" />
                <span class="input-group-addon">
                    <span class="fa fa-calendar"></span>
                </span>
            </div>
        </div>
        <div class="col-md-7">
            @Html.Label("Hasta", new { @for = "slpCode", @class = "control-label" })
            <select class="form-control required" id="slpCode" name="slpCode" style="max-width:100%">
                <option value="" disabled selected></option>
            </select>
        </div>
        <div class="col-md-1">
            <label class="control-label" style="color:white">.</label>
            <input type="button" value="Buscar" id="btnSearch" class="btn btn-info" />
        </div>
    </div>
</div>

<div class="row ordersMap">

</div>

